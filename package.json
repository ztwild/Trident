{
  "name": "test-app",
  "version": "0.1.0",
  "author": "Zach Wild",
  "private": true,
  "main": "public/electron.js",
  "homepage": "./",
  "build": {
    "appId": "com.example.electron-cra",
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    }
  },
  "dependencies": {
    "axios": "^0.18.0",
    "body-parser": "^1.18.3",
    "concurrently": "^4.0.1",
    "electron-is-dev": "^1.0.1",
    "express": "^4.16.3",
    "jquery": "^3.3.1",
    "mongodb": "^3.1.6",
    "oauthio-web": "^0.6.2",
    "react": "^16.4.2",
    "react-bootstrap": "^0.32.4",
    "react-dom": "^16.4.2",
    "react-redux": "^5.0.7",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-scripts": "1.1.4",
    "reactstrap": "^6.3.1",
    "readline": "^1.3.0",
    "redux": "^4.0.0",
    "redux-saga": "^0.16.0",
    "socket.io": "^2.1.1",
    "toastr": "^2.1.4",
    "wait-on": "^3.1.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "electron": "electron .",
    "electron-dev": "concurrently \"BROWSER=none npm start\" \"wait-on http://localhost:3000 && electron .\"",
    "prepack": "npm run build",
    "pack": "build --dir",
    "dist": "build"
  },
  "win": {
    "target": "NSIS"
  },
  "devDependencies": {
    "electron": "^3.0.4",
    "electron-builder": "^20.28.4"
  }
